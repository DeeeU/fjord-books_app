<h1><%= t('views.common.title_index', name: t('views.items.user'))%></h1>

<ul class="users">
  <% @users.each do |user| %>
    <li>
     |  <%= user.id %>  |  <%= user.name %>  |  <%= user.email %>
      <%= link_to t('views.common.show') , user_path(user.id) %>
    </li>
  <% end %>
  <%= paginate @users %>
</ul>
